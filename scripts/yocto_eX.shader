// LIGHTS

// blue horizontal
textures/yocto_eX/eX_light_u201_add_b
{
	qer_editorimage textures/yocto_eX/eX_light_u201_d.tga
	diffuseMap textures/yocto_eX/eX_light_u201_d.tga
	normalMap addnormals(textures/yocto_eX/eX_light_u201_local.tga, heightmap(textures/yocto_eX/eX_light_u201_h.tga, 1))
	specularMap textures/yocto_eX/eX_light_u201_s.tga
	surfaceparm nomarks
	q3map_surfacelight 5001
	q3map_lightRGB .4 .7 1
	{
		map textures/yocto_eX/eX_light_u201_add_b.tga
		blendfunc GL_ONE GL_ONE
	}
}

